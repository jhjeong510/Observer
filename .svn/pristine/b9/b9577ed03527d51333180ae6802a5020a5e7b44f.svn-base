.custom-context-menu {
  display: none;
  position: absolute;
  box-sizing: border-box;
  min-height: 34px;
  min-width: 11%;
  background-color: #f8f8f8;
  box-shadow: 0 0 1px 2px lightgrey;
  z-index: 999999999;
}

.custom-context-menu ul {
  list-style: none;
  padding: 0;
  background-color: transparent;
  margin-bottom: 0;
}

.custom-context-menu li {
  color: #140a00;
  font-weight: 500;
  padding: 3px 5px;
  cursor: pointer;
}

.custom-context-menu li:hover {
  background-color: #0B9BCD;
}

