html,
body {
	margin: 0;
	padding: 0;
	height: 100%;
}

.layout {
	/* position: fixed;
    top: 80px;
    left:0;
    height:100%;
    width :100%;
    display: flex;
	right: 465px;
	width: calc(100% - 465px) */

	display: flex;
	height: calc(100%);
	width: calc(100%);
}

.center {
	position: relative;
	width: 100%;
	height: 100%;
	z-index: 99;
	background-color: rgba(255, 33, 33, 0);
	overflow: hidden;
}

.mainContainer {
	display: flex;
	height: calc(100% - 200px);
	width: calc(100%);
}

.separator {
	height: 1px;
	background: #dedede;
	margin: 2px 0px 2px 0px;
}


.box {
	position: absolute;
	bottom: 0;
	right: 0;
	width: 300px;
	max-height: 250px;
	z-index: 99999999999;
	overflow-y: scroll;
}


.popup {
	position: absolute;
	bottom: 0;
	right: 0;
	width: 300px;
	max-height: 250px;
	z-index: 99999;
	overflow-y: scroll;
}

.popupTab {
	display: flex;
	height: 35px;
	background-color: #374357;
	color: white;
}

.history-tab {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	text-align: center;
}

.history-tab-border {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	text-align: center;
	background-color: #171c24;
	color: white;
}

.layer-tab {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	text-align: center;
}

.layer-tab-border {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	text-align: center;
	background-color: #171c24;
	color: white;
}

.info-tab {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	text-align: center;
}

.info-tab-border {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 100%;
	text-align: center;
	background-color: #171c24;
	color: white;
}


.popup-tab:hover {
	cursor: pointer;
}

.popupContent {
	overflow-x: hidden;
	overflow-y: auto;
	background-color: #f8f5f1;
	padding: 5%;

}