						<div className={styles.camArea}>
							<div className={styles.camArea1}>
								<div className={styles.camArea_}>
									<img src={testImage} style={{ width: '560px', height: '315px', objectFit: 'fill' }}></img>
								</div>
							</div>
						</div>

						<div className={styles.camArea}>
							<div className={styles.camArea2}>
								<div className={styles.camArea_}>
									영역2
								</div>
							</div>
						</div>
					</div>
